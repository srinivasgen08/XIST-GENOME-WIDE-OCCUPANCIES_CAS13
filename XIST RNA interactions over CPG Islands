setwd("C:/Users/Precision Medicine/Desktop/CHIP SEQ_GENE WIZ/ALL INTERVAL FILES/CHROMOSOME WISE SORTING OF BED FILES/TESTING TEMPORARY")

library("GenomicRanges")
library("ChIPpeakAnno")
library("IRanges")
library("GenomicFeatures")
library(VennDiagram)
library(dplyr)
library("readxl")

Chr1_CPG
write.table(Chr1_CPG, file = "Chr1_CPG", sep = "\t",row.names = FALSE)
Chr1_PA.G.GFP

Chr2_PA.G.H3K27

A1 = GRanges(seqnames=Rle(Chr1_CPG[,1]),
             ranges=IRanges(Chr1_CPG[,2], Chr1_CPG[,3]))
A1
A2= GRanges(seqnames=Rle(Chr1_PA.G.GFP[,1]),ranges=IRanges(Chr1_PA.G.GFP[,2], Chr1_PA.G.GFP[,3]))

A3=GRanges(seqnames=Rle(Chr2_PA.G.H3K27[,1]),ranges=IRanges(Chr2_PA.G.H3K27[,2], Chr2_PA.G.H3K27[,3]))

A3
l=mean(A1 %over% A2)
l
m=mean(A1 %over% A3)
m
n=mean(A2 %over% A3)
n

data.frame(c(l,m,n), c("a","b", "c"))

k="lu"
d="mn"
E="OP"

tp=c(k,d,E)
tp

oo= c(l,m,n)
oo

kk=c("PAG","PGE","PEG")



data.frame(kk, oo,  oo,  oo, oo,  oo,  oo)

data.frame(c(l,m,n))



ABvenn <- makeVennDiagram(Peaks=list(A1,A2),NameOfPeaks=c("A1", "A2"))

ACvenn<- makeVennDiagram(Peaks=list(A1,A3),NameOfPeaks=c("A1", "A3"))
BDvenn<- makeVennDiagram(Peaks=list(A2,A3),NameOfPeaks=c("A2", "A3"))

ABvenn
ACvenn
BDvenn

ABvenn

write.table(ABvenn, "ABvenn.txt")
as.data.frame(ABvenn, header=TRUE)

typeof(ABvenn)
class(ABvenn)



